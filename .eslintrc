{
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "browser": true
  },
  "plugins": ["react", "import"],
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "google",
    "plugin:import/errors",
    "plugin:import/warnings"
  ],
  "rules": {
    "max-len": [
      "error",
      140
    ],
    "no-multi-spaces": "error",
    "camelcase": "error",
    "require-jsdoc": "off",
    "comma-dangle": ["error", "never"]
  }
}
